    <!-- All Jquery -->
    <!-- ============================================================== -->
    <script>
var pendingbtns = document.getElementsByClassName("pendingbtns")
for(btn of pendingbtns){
  btn.addEventListener("click", function(event){
        //  event.preventDefault();
        console.log(event.target.parentElement.id.substring(1),"hi doc")
         fetch("/doctor/update", {
            method: "POST",
            body: JSON.stringify({
                _csrf: "<%= csrfToken %>",
                cid: event.target.parentElement.id.substring(1),
                // email: "<%= doctor[0].email %>"

            }),
            headers: {
                "Content-type": "application/json; charset=UTF-8"
            }
        })
          
        // Converting to JSON
        .then(response =>console.log(response) )
        .catch(err=>console.log(err))
          // console.log(event.target.parentElement);
        document.getElementById(event.target.parentElement.id.substring(1)).style.display="none";
         window.location.reload()
      });
      

}
var pendingmails = document.getElementsByClassName("pendingmails")

for(btn of pendingmails){

      btn.addEventListener("click", function(event){
        //  event.preventDefault();

       
         fetch("/doctor/sendmail", {
            method: "POST",
            body: JSON.stringify({
                _csrf: "<%= csrfToken %>",
                cid: event.target.parentElement.id.substring(1),
                demail: "<%= doctor[0].email %>",
                
                // email: "<%= doctor[0].email %>"
            }),
            headers: {
                "Content-type": "application/json; charset=UTF-8"
            }
        })
          
        // Converting to JSON
        .then(response =>{console.log(response) 
        alert("Mail has been Successfully sent")
        }
        )
        .catch(err=>console.log(err))
        
      });
    }
      document.getElementById("btnlt").addEventListener("click", function(event){
         fetch("/doctor/logout", {
            method: "GET",
            headers: {
                "Content-type": "application/json; charset=UTF-8"
            }
        })
        .then(response =>console.log(response) )
        .catch(err=>console.log(err))
         window.location.reload()
      });

            </script>
