        <!-- ============================================================== -->
        <!-- Container fluid  -->
        <!-- ============================================================== -->
        <div class="container-fluid">
          <!-- ============================================================== -->
          <!-- Three charts -->
          <!-- ============================================================== -->
          <div class="row justify-content-center">
            <div class="col-lg-4 col-md-12">
              <div class="white-box analytics-info">
                <h3 class="box-title">Total Consultants</h3>
                <ul class="list-inline two-part d-flex align-items-center mb-0">
                  <li>
                    <div id="sparklinedash">
                      <canvas
                        width="67"
                        height="30"
                        style="
                          display: inline-block;
                          width: 67px;
                          height: 30px;
                          vertical-align: top;
                        "
                      ></canvas>
                    </div>
                  </li>
                  <li class="ms-auto">
                    <span class="counter text-success"><%=consultants.length%></span>
                  </li>
                </ul>
              </div>
            </div>
            <div class="col-lg-4 col-md-12">
              <div class="white-box analytics-info">
                <h3 class="box-title">Pending Consultants</h3>
                <ul class="list-inline two-part d-flex align-items-center mb-0">
                  <li>
                    <div id="sparklinedash2">
                      <canvas
                        width="67"
                        height="30"
                        style="
                          display: inline-block;
                          width: 67px;
                          height: 30px;
                          vertical-align: top;
                        "
                      ></canvas>
                    </div>
                  </li>
                  <li class="ms-auto">
                    <span class="counter text-purple">
                      <% var n=0;for(var i=0;i<consultants.length;i++){ 
                         if(consultants[i].status=="pending"){ 
                          n++;}} %>
                       <%= n%> 
                    </span>
                  </li>
                </ul>
              </div>
            </div>
            <div class="col-lg-4 col-md-12">
              <div class="white-box analytics-info">
                <h3 class="box-title">Completed Consultants</h3>
                <ul class="list-inline two-part d-flex align-items-center mb-0">
                  <li>
                    <div id="sparklinedash3">
                      <canvas
                        width="67"
                        height="30"
                        style="
                          display: inline-block;
                          width: 67px;
                          height: 30px;
                          vertical-align: top;
                        "
                      ></canvas>
                    </div>
                  </li>
                  <li class="ms-auto">
                    <span class="counter text-info"><%=consultants.length-n%></span>
                  </li>
                </ul>
              </div>
            </div>
          </div>
          <!-- ============================================================== -->
          <!-- PRODUCTS YEARLY SALES -->
          <!-- ============================================================== -->
          <div class="row">
            <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
          
           
            <!-- </div>
          </div> -->
          <!-- ============================================================== -->
          <!-- RECENT SALES -->
          <!-- ============================================================== -->
          <div class="row">
            <div class="col-md-12 col-lg-12 col-sm-12">
              <div class="white-box">
                <div class="d-md-flex mb-3">
                  <h3 class="box-title mb-0">Pending Consultants</h3>
                 
                </div>
                <div class="table-responsive">
                  <table class="table no-wrap">
                    <thead>
                      <tr>
                        <th class="border-top-0">#</th>
                        <th class="border-top-0">Name</th>
                        <th class="border-top-0">Email</th>
                        <th class="border-top-0">Number</th>
                        <th class="border-top-0">Time Slot</th>
                        <th class="border-top-0">Problem Type</th>
                        <th class="border-top-0">Send Email</th>
                        <th class="border-top-0">Status</th>
                      </tr>
                    </thead>
                   <%if(n>0){%>
                    <% for(var j=0;j<consultants.length;j++){ %>
                      <% if(consultants[j].status=="pending"){ %>
                        <div>
                          <tr id="<%= consultants[j]._id %>">
                            <td><%=j+1%></td>
                            
                            <td><%= consultants[j].name %></td>
                            <td><%= consultants[j].email %></td>
                            <td><%= consultants[j].number %></td>
                            <td><%= consultants[j].timeslot %></td>
                            <td><%= consultants[j].problemtype %></td>
                            <td id="y<%= consultants[j]._id %>"><button type="submit" class="pendingmails" id="btncd<%=j%>">Send email</button></td>
                             <td id="x<%= consultants[j]._id %>">
                               <button type="submit" class="pendingbtns" id="btnst<%=j%>">completed</button>
                             </td> 
                          </tr>
                        </div>
                          <%}%>
                        <%}%>
                        <%}else{%>
                          <div>
                            <tr>
                              <td></td>
                              <td></td>
                              <td></td>
                              <td></td>
                              <td>No Data Found</td>
                              <td></td>
                              <td></td>
                              
                            </tr>
                          </div>

                        <%}%>
                          
                  </table>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-12 col-lg-12 col-sm-12">
              <div class="white-box">
                <div class="d-md-flex mb-3">
                  <h3 class="box-title mb-0">Completed Consultants</h3>
                 
                </div>
                <div class="table-responsive">
                  <table class="table no-wrap">
                    <thead>
                      <tr>
                        <th class="border-top-0">#</th>
                        <th class="border-top-0">Name</th>
                        <th class="border-top-0">Email</th>
                        <th class="border-top-0">Number</th>
                        <th class="border-top-0">Time Slot</th>
                        <th class="border-top-0">Problem Type</th>
                        <th class="border-top-0">Status</th>
                      </tr>
                    </thead>
          
                    <% for(var i=0;i<consultants.length;i++){ %>
                      <% if(consultants[i].status=="completed"){ %>
                        <div>
                          <tr id="<%= consultants[i]._id %>">
                            <td><%=i+1%></td>
                            <td><%= consultants[i].name %></td>
                            <td><%= consultants[i].email %></td>
                            <td><%= consultants[i].number %></td>
                            <td><%= consultants[i].timeslot %></td>
                            <td><%= consultants[i].problemtype %></td>
                            <td>completed</td>
                             <!-- <td id="x<%= consultants[i].email %>">
                               <button type=""  id="btnst">completed</button>
                             </td>  -->
                          </tr>
                        </div>
                          <%}%>
                        <%}%>
                  </table>
                </div>
              </div>
            </div>
          </div>
         
          <!-- ============================================================== -->
          <!-- Recent Comments -->
          <!-- ============================================================== -->
         
        <!-- ============================================================== -->
        <!-- End Container fluid  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- footer -->
        <!-- ============================================================== -->
      
        <!-- ============================================================== -->
        <!-- End footer -->
        <!-- ============================================================== -->
      </div>
      <!-- ============================================================== -->
      <!-- End Page wrapper  -->
      <!-- ============================================================== -->
    </div>
